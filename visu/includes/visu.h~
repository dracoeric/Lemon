#ifndef VISU_H
# define VISU_H

# define B_SIZE 16364

typedef	struct	s_room
{
	char	*room;
	int		x;
	int		y;
}				t_room;

typedef	struct	s_file
{
	char			*buf;
	int				size;
	struct s_file	*next;
}				t_file;

